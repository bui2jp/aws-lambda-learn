{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "common/*": ["../common/*"]
      },
      "target": "es2020",
      "strict": true,
      "preserveConstEnums": true,
      "noEmit": false,
      "module":"esnext",
      "moduleResolution":"node",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "inlineSourceMap": true
    },
    "exclude": ["node_modules", "**/*.test.ts"]
  }